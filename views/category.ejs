<!-- form -->
<h1>Create a Category</h1>


<form action="/category" method="post">
  <div class="row">
    <div class="col s12">
      <div class="row">
        <div class="input-field col s12">
          <input type="text" placeholder="Category Text" id="label" class="validate" name="text">
          <label for="label">Category Text</label>
        </div>
      </div>
    </div>
  </div>
  <div>
    <button class="btn waves-effect waves-light" type="submit" name="action">Submit
                <i class="mdi-content-send"></i>
            </button>
  </div>
</form>